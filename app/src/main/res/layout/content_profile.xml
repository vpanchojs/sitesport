<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary"
    android:layout_marginBottom="60dp"
    android:fillViewport="true"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".profile.ui.ProfileActivity"
    tools:showIn="@layout/activity_profile">

    <android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:text="Horario de atención"
            android:textColor="#fff"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_chainStyle="spread_inside" />

        <View
            android:id="@+id/textView4"
            android:layout_width="0dp"
            android:layout_height="0.1dp"
            android:layout_marginEnd="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="10dp"
            android:background="#1effffff"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView2" />

        <ImageButton
            android:id="@+id/btnShowBusinessHours"
            android:layout_width="@dimen/size_btn_profile"
            android:layout_height="@dimen/size_btn_profile"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/bg_ibtn_profile"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView4"
            app:srcCompat="@drawable/ic_clock"
            android:tint="@color/white"/>

        <TextView
            android:id="@+id/tvStateEnterprise"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:layout_marginEnd="16dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="Abierto"
            android:textColor="#fff"
            app:layout_constraintBottom_toBottomOf="@+id/btnShowBusinessHours"
            app:layout_constraintStart_toEndOf="@+id/btnShowBusinessHours"
            app:layout_constraintTop_toTopOf="@+id/btnShowBusinessHours" />


        <!-- HORARIO DE ATENCION -->


        <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:layout_marginEnd="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:text="Precio (hora)"
            android:textColor="#fff"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btnShowBusinessHours" />

        <View
            android:id="@+id/textView7"
            android:layout_width="0dp"
            android:layout_height="0.1dp"
            android:layout_marginEnd="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="10dp"
            android:background="#1effffff"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView6" />

        <ImageButton
            android:id="@+id/btnShowRateDay"
            android:layout_width="@dimen/size_btn_profile"
            android:layout_height="@dimen/size_btn_profile"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/bg_ibtn_profile"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView7"
            app:srcCompat="@drawable/ic_sun" />

        <TextView
            android:id="@+id/tvPriceDayStandar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="$24.00"
            android:textColor="#fff"
            app:layout_constraintBottom_toBottomOf="@+id/btnShowRateDay"
            app:layout_constraintStart_toEndOf="@+id/btnShowRateDay"
            app:layout_constraintTop_toTopOf="@+id/btnShowRateDay" />

        <ImageButton
            android:id="@+id/btnShowRateNight"
            android:layout_width="@dimen/size_btn_profile"
            android:layout_height="@dimen/size_btn_profile"
            android:layout_marginStart="32dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/bg_ibtn_profile"
            app:layout_constraintStart_toEndOf="@+id/tvPriceDayStandar"
            app:layout_constraintTop_toBottomOf="@+id/textView7"
            app:srcCompat="@drawable/ic_moon" />


        <TextView
            android:id="@+id/tvPriceNightStandar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:layout_marginEnd="16dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="$24.00"
            android:textColor="#fff"
            app:layout_constraintBottom_toBottomOf="@+id/btnShowRateNight"
            app:layout_constraintStart_toEndOf="@+id/btnShowRateNight"
            app:layout_constraintTop_toTopOf="@+id/btnShowRateNight" />

        <!-- COMUNICACION -->


        <TextView
            android:id="@+id/tvTimeSabadoToDomingo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:layout_marginEnd="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:text="Contáctos"
            android:textColor="#fff"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btnShowRateDay" />

        <View
            android:id="@+id/textView11"
            android:layout_width="0dp"
            android:layout_height="0.1dp"
            android:layout_marginEnd="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="10dp"
            android:background="#1effffff"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tvTimeSabadoToDomingo" />

        <ImageButton
            android:id="@+id/ibtnPhone"
            android:layout_width="@dimen/size_btn_profile"
            android:layout_height="@dimen/size_btn_profile"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/bg_ibtn_profile"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView11"
            app:srcCompat="@drawable/ic_phone" />

        <ImageButton
            android:id="@+id/ibtnWhatsapp"
            android:layout_width="@dimen/size_btn_profile"
            android:layout_height="@dimen/size_btn_profile"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/bg_ibtn_profile"
            app:layout_constraintStart_toEndOf="@+id/ibtnPhone"
            app:layout_constraintTop_toBottomOf="@+id/textView11"
            app:srcCompat="@drawable/ic_whastapp" />

        <ImageButton
            android:id="@+id/ibtnFacebook"
            android:layout_width="@dimen/size_btn_profile"
            android:layout_height="@dimen/size_btn_profile"
            android:layout_marginEnd="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/bg_ibtn_profile"
            app:layout_constraintStart_toEndOf="@+id/ibtnWhatsapp"
            app:layout_constraintTop_toBottomOf="@+id/textView11"
            app:srcCompat="@drawable/ic_facebook" />

        <android.support.constraint.ConstraintLayout
            android:id="@+id/constraintLayout3"
            android:layout_width="0dp"
            android:layout_height="183dp"
            android:layout_marginEnd="2dp"
            android:layout_marginStart="2dp"
            android:layout_marginTop="16dp"
            android:foreground="@drawable/bg_corner_map"
            android:padding="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ibtnFacebook">

            <com.mapbox.mapboxsdk.maps.MapView
                android:id="@+id/mvProfile"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="#fff"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:mapbox_cameraTargetLat="40.73581"
                app:mapbox_cameraTargetLng="-73.99155"
                app:mapbox_cameraZoom="8"
                app:mapbox_styleUrl="mapbox://styles/mapbox/streets-v10">

            </com.mapbox.mapboxsdk.maps.MapView>

            <!--<ImageButton
                android:id="@+id/ibtnRoute"
                android:layout_width="@dimen/size_btn_profile"
                android:layout_height="@dimen/size_btn_profile"
                android:layout_marginBottom="8dp"
                android:layout_marginEnd="8dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:background="@drawable/bg_ibtn_secondary"
                android:src="@drawable/ic_run_man"
                android:tint="@color/colorPrimary"/>-->

        </android.support.constraint.ConstraintLayout>

    </android.support.constraint.ConstraintLayout>

</android.support.v4.widget.NestedScrollView>

